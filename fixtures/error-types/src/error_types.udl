namespace error_types {
    [Throws=ErrorInterface]
    void oops();

    ErrorInterface get_error(string message);

    [Throws=EnumError]
    void simple();

};

[Traits=(Debug, Display)]
interface ErrorInterface {
    sequence<string> chain();
    string? link(u64 index);
};

// A regular enum error.
[Error]
enum EnumError {
    "Oops"
};
